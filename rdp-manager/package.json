{
  "name": "rdp-manager",
  "version": "1.0.11",
  "description": "RDP Connection Manager",
  "author": "T3rr0or",
  "publisher": "T3rr0or",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --publish always"
  },
  "build": {
    "appId": "com.t3rr0or.rdpmanager",
    "productName": "RDP Manager",
    "copyright": "Copyright Â© 2024 T3rr0or",
    "asar": true,
    "forceCodeSigning": false,
    "publish": [
      {
        "provider": "github",
        "owner": "T3rr0or",
        "repo": "RDP-Manager"
      }
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "build/icon.ico",
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "license": "../LICENSE",
      "artifactName": "RDP-Manager-Setup-${version}.${ext}"
    }
  },
  "dependencies": {
    "electron-log": "^5.0.1",
    "electron-store": "^8.0.1",
    "electron-updater": "^6.1.7"
  },
  "devDependencies": {
    "@types/electron-store": "^1.3.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  }
}
